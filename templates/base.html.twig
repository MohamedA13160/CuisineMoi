								<li class="{% if current_menu_profil is defined and relative_path("profile")%}active{% endif %}">
								<a href="{{path('profile', {id : app.user.id})}}">Profile</a>

								</li>

							{% endif %}

							<li class="{% if current_menu_mail is defined and relative_path("contact")%}active{% endif %}">
								<a href="{{path('contact')}}">Nous contacter</a>

								</li>

							{% if app.user == null %}
								<li class="{% if form_loge is defined and relative_path("login")%} active {% endif %}">

									<a href="{{ path('login')}}">Se Connecter</a>
								</li>
								<li class="{% if form_create is defined and relative_path("sign.up")%}active{% endif %}">

									<a href="{{ path('sign.up')}}">S'inscrire</a>
								</li>

                                  
                            {% endif %}

							{% if is_granted('ROLE_USER') and is_granted('ROLE_ADMIN') == null %}
                                      
								<li class="">
									<a href="{{ path('logout')}}" class="nav-link">Se deconnecter</a>
								</li>

							{% endif %}

							{% if is_granted('ROLE_ADMIN') %}

								<li class="">
									<a href="{{ path('admin.index')}}" class="nav-link">Admin</a>
								</li>

                            {% endif %}
                                     


                                </ul>
